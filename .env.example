# Database Configuration (REQUIRED)
# PostgreSQL connection string
# Format: postgresql://username:password@hostname:port/database_name
# Example: postgresql://user:pass@34.88.123.45:5432/jobsdb
DATABASE_URL=postgresql://user:pass@host:5432/jobsdb

# Scraper Configuration
# FINN location code (Bergen: 2.20001.22046.20220)
FINN_LOCATION=2.20001.22046.20220

# NAV location (County and Municipality)
NAV_COUNTY=VESTLAND
NAV_MUNICIPAL=VESTLAND.BERGEN

# Max results per keyword (prevents overwhelming database with duplicates)
MAX_JOBS_PER_KEYWORD=20

# Max keywords to search (0 = all keywords, useful for testing with fewer keywords)
MAX_KEYWORDS=0

# Keywords (comma-separated, optional - defaults to full list in config.py)
# KEYWORDS=python,sql,machine learning,data scientist

# Logging
LOG_LEVEL=INFO
